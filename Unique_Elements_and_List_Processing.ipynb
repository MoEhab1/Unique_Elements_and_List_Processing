    "nums = input()\n",
    "\n",
    "numList = set()\n",
    "for i in nums:\n",
    "    if i not in numList:\n",
    "        numList.add(int(i))\n",
    "print(\"(unique elements): \", numList)\n",
    "\n",
    "SqEv = []\n",
    "for i in numList:\n",
    "    if i %2 == 0:\n",
    "        SqEv.append(i*i)\n",
    "print(\"(squares of even numbers):\", SqEv)"
